{
	"name": "@calmdownval/mime",
	"version": "1.1.4",
	"type": "module",
	"main": "src/index.mjs",
	"types": "src/index.d.ts",
	"license": "GPL-3.0-or-later",
	"description": "MIME type parser",
	"keywords": [
		"MIME",
		"ContentType",
		"parser"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/vaclavlahuta/mime.git"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"devDependencies": {
		"c8": "^5.0.1",
		"codacy-coverage": "^3.4.0",
		"cross-env": "^5.2.0",
		"dotenv": "^8.0.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4"
	},
	"scripts": {
		"test": "cross-env NODE_OPTIONS=\"--experimental-modules\" mocha --delay script/mocha.cjs",
		"test:coverage": "cross-env NODE_OPTIONS=\"--experimental-modules\" c8 --clean --check-coverage mocha --delay script/mocha.cjs",
		"coverage": "npm run test:coverage && c8 report --reporter=lcovonly --include=**/src/**"
	}
}
